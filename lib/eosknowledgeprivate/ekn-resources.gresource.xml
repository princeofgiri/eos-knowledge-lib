<?xml version="1.0" encoding="UTF-8"?>
<gresources>
  <gresource prefix="/com/endlessm/knowledge-private">

    <!-- Do not compress files that will be frequently used.
         Since we are linking staticly with this resource the compressed data
         would end up in the text area of the executable and the uncompressed
         one in the heap and we rather waste some disk space instead of memory.
      -->
    <file preprocess="xml-stripblanks">EknRuntimeDocumentViewer.ui</file>
    <file preprocess="xml-stripblanks">EknMediaBin.ui</file>
    <file>ekn-media-bin.css</file>
    <file>broadway/broadway.js</file>
    <file>broadway/rawinflate.min.js</file>
    <file>broadway/client.html</file>
  </gresource>

  <gresource prefix="/com/endlessm/knowledge-private/scalable/actions">
    <file preprocess="xml-stripblanks">ekn-media-bin-play-symbolic.svg</file>
    <file preprocess="xml-stripblanks">ekn-media-bin-pause-symbolic.svg</file>
    <file preprocess="xml-stripblanks">ekn-media-bin-fullscreen-symbolic.svg</file>
    <file preprocess="xml-stripblanks">ekn-media-bin-restore-symbolic.svg</file>
    <file preprocess="xml-stripblanks">ekn-media-bin-volume-muted-symbolic.svg</file>
    <file preprocess="xml-stripblanks">ekn-media-bin-volume-low-symbolic.svg</file>
    <file preprocess="xml-stripblanks">ekn-media-bin-volume-medium-symbolic.svg</file>
    <file preprocess="xml-stripblanks">ekn-media-bin-volume-high-symbolic.svg</file>
  </gresource>
</gresources>
